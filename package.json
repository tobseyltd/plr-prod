{
	"name": "svelte-version",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"gen:types": "pnpx supabase gen types typescript --local > src/lib/supabase-types.ts && prettier --write src/lib/supabase-types.ts",
		"seed": "tsx --tsconfig scripts/tsconfig.json scripts/seed.ts",
		"stripe:webhooks": "stripe listen --forward-to localhost:5173/api/stripe/webhook"
	},
	"devDependencies": {
		"@faker-js/faker": "^8.4.0",
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-vercel": "^5.1.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/detect-port": "^1.3.5",
		"@types/eslint": "8.56.0",
		"@types/pg": "^8.11.0",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"detect-port": "^1.5.1",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"pg": "^8.11.3",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"stripe-event-types": "^3.1.0",
		"supabase": "^1.136.3",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"sveltekit-superforms": "2.0.0-alpha.45",
		"tslib": "^2.4.1",
		"tsx": "^4.7.0",
		"typescript": "^5.0.0",
		"vite": "^5.0.3",
		"vitest": "^1.2.0",
		"zod": "^3.22.4"
	},
	"type": "module",
	"dependencies": {
		"@supabase/ssr": "^0.1.0",
		"@supabase/supabase-js": "^2.39.3",
		"dotenv": "^16.4.1",
		"lucide-svelte": "^0.321.0",
		"stripe": "^14.16.0",
		"svelte-french-toast": "^1.2.0"
	}
}
